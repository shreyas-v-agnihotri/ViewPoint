# Uncomment the next line to define a global platform for your project
platform :ios, '12.1'

target 'ViewPoint' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # UI and Networking Pods
  pod 'SVProgressHUD'
  pod 'Kingfisher'
  pod 'AlamofireImage'
  pod 'Navigation-Toolbar'
  
  # Authentication pods
  pod 'GoogleSignIn'
  pod 'Firebase/Auth'

  # Database pods
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'
  


end
